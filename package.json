{
  "name": "@zimbra/zm-x-api-js-client",
  "amdName": "zm-x-api-js-client",
  "version": "1.0.0",
  "description": "Zimbra JS API Client is a GraphQL client for making requests against the Zimbra SOAP API.",
  "main": "dist/zm-x-api-js-client.js",
  "source": "index.ts",
  "module": "dist/zm-x-api-js-client.esm.js",
  "umd:main": "dist/zm-x-api-js-client.umd.js",
  "scripts": {
    "build:ts": "tsc --emitDeclarationOnly",
    "build": "npm run rollup -s && npm run minify -s && npm run size -s",
    "lint": "tslint --project .",
    "eslint": "eslint src --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "rollup:cjs": "rollup -c rollup.config.js -m -f cjs -n $npm_package_amdName $npm_package_source -o $npm_package_main",
    "rollup:umd": "rollup -c rollup.config.js -m -f umd -n $npm_package_amdName $npm_package_source -o $npm_package_umd_main",
    "rollup:esm": "rollup -c rollup.config.js -m -f es --environment FORMAT:es -n $npm_package_amdName $npm_package_source -o $npm_package_module",
    "rollup": "rollup -c rollup.config.js -m -f umd -n $npm_package_amdName src/index.ts -o $npm_package_main",
    "minify": "uglifyjs $npm_package_main -cm -o $npm_package_main -p relative --in-source-map $npm_package_main.map --source-map $npm_package_main.map",
    "size": "echo \"Gzipped Size: $(gzip-size $npm_package_main | pretty-bytes)\"",
    "release": "npm run test && npm run build -s && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "zimbra",
    "zm-x-web",
    "graphql"
  ],
  "extends": [
    "eslint-config-synacor"
  ],
  "repository": "Zimbra/zm-x-api-js-client",
  "license": "BSD-3-Clause",
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-link": "^1.2.1",
    "apollo-link-batch": "^1.1.2",
    "apollo-link-error": "^1.0.7",
    "dataloader": "^1.4.0",
    "graphql-tools": "^2.23.1",
    "lodash-es": "^4.17.7",
    "mitt": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-external-helpers": "^7.0.0-beta.42",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-typescript": "^7.0.0-beta.42",
    "@types/graphql": "^0.12.6",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^9.6.0",
    "@types/recompose": "^0.24.6",
    "@types/whatwg-fetch": "0.0.33",
    "apollo-codegen": "^0.19.0",
    "eslint": "^4.19.1",
    "eslint-config-synacor": "^3.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "prettier-eslint": "^8.8.1",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^4.0.0-beta.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-graphql": "^0.1.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-typescript2": "^0.12.0",
    "shelljs": "^0.8.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.2"
  },
  "peerDependencies": {
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0"
  }
}
